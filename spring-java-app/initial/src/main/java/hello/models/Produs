package hello;

import java.util.List;
import java.util.ArrayList;



public class Produs {
  private String nume;
  private int id_produs;
  private int pret_produs;
  private String data_exp;


  

public Produs() {}

  

public Produs(int id_produs, String nume,int pret_produs,String data_exp) {
      this.nume = nume;
      this.id_produs = id_produs;
      this.pret_produs=pret_produs;
      this.data_exp=data_exp;
      
  }

  

public String getnume() {
      return this.nume;
  }

  

public int getid_produs() {
    return this.id_produs;
  }
  
public int getpret_produs() {
    return this.pret_produs;
  }


public String getdata() {
      return this.data_exp;
  }

  
  
public int setid(int id){
	 return this.id_produs = id;
  }
  
  

public String setnume(String nume){
	return  this.nume = nume;
  }
}
