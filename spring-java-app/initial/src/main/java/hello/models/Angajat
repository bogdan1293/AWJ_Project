package hello;

import java.util.List;
import java.util.ArrayList;



public class angajat {
  private String nume;
  private int id_ang;
  private String post;
  private int salariu;


  

public angajat() {}

  

public angajat(int id_ang, String nume,string post,int salariu) {
      this.nume = nume;
      this.id_ang = id_ang;
      this.post=post;
      this.salariu=salariu;
      
  }

  

public String getnume() {
      return this.nume;
  }

  

public int getid_ang() {
    return this.id_ang;
  }
  
public int getpost() {
    return this.post;
  }


public String getsalariu() {
      return this.salariu;
  }

  
  
public int setid(int id){
	 return this.id_ang = id;
  }
  
  

public String setnume(String nume){
	return  this.nume = nume;
  }
}
